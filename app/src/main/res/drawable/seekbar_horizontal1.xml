<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#fff" />
            <stroke
                android:width="1px"
                android:color="#fff" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#4285F6" />
                <stroke
                    android:width="1px"
                    android:color="#4285F6" />

            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>

                <solid android:color="#4285F6" />
                <stroke
                    android:width="1px"
                    android:color="#4285F6" />

            </shape>
        </clip>
    </item>
    <!--<item android:id="@android:id/progress" android:drawable="@drawable/seek_bar_progress">
    </item>-->

</layer-list>